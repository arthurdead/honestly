#ifndef __PCH__
	#pragma once
#endif

#include "__private/version_prefixes.hpp"

#pragma push_macro("__has_include")

#ifndef __has_include
	#define __has_include(x) (0)
#endif

#ifndef __CPPCHECK__
	#if __has_include(<features.h>)
		#define __CTL_HAS_FEATURES_H
	#endif

	#if __has_include(<yvals.h>)
		#define __CTL_HAS_YVALS_H
	#endif
#endif

#pragma pop_macro("__has_include")

#ifdef __CTL_HAS_FEATURES_H
	#if defined _FORTIFY_SOURCE && _FORTIFY_SOURCE > 0
		#pragma push_macro("__OPTIMIZE__")
		#ifndef __OPTIMIZE__
			#define __OPTIMIZE__ 1
		#endif
	#endif

	#include <features.h>

	#if defined _FORTIFY_SOURCE && _FORTIFY_SOURCE > 0
		#pragma pop_macro("__OPTIMIZE__")
	#endif
#endif

#ifdef __CTL_HAS_YVALS_H
	#include <yvals.h>
#endif

#undef __CTL_HAS_FEATURES_H
#undef __CTL_HAS_YVALS_H

#include <version>

#include "__private/version_compiler.hpp"
#include "__private/version_libcpp.hpp"
#include "__private/version_libc.hpp"
#include "__private/version_debuglevel.hpp"
#include "__private/version_postfixes.hpp"
#include "__private/version_cpp.hpp"
#include "__private/version_c.hpp"
#include "__private/version_requirements.hpp"
#include "__private/version_os.hpp"
#include "__private/version_architecture.hpp"
#include "__private/version_compiler_builtins.hpp"
